---
 import { type APIXSpaceXResponse, type Doc } from '../../types/api'
 import Card from '../../components/Card.astro';
import { getLatestLaunches } from '../../lib/services/spacex.ts';


const  docs = await getLatestLaunches();

---

<div class="grid gap-8 sm:grid-cols-2 lg:grid-cols-3">
    {
        docs.map( (doc: Doc) => (
           /*<article>
                <img src={doc.links.patch.small} alt=""/>
                <h2>Launch #{doc.id}</h2>
            </article>*/
            <Card id={doc.id} img={doc.links.patch.small} success={doc.success} flightNumber={doc.flight_number} details={doc.details}/>
        ))
    }
</div>